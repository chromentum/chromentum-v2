{
	"name": "Chromentum",
	"version": "0.0.0",
	"description": "An awesome new browser extension",
	"manifest_version": 2,
	"minimum_chrome_version": "80",
	"applications": {
		"gecko": {
			"id": "awesome-extension@notlmn.github.io",
			"strict_min_version": "80.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	},
	"chrome_url_overrides": {
		"newtab": "tab.html"
	}
}
